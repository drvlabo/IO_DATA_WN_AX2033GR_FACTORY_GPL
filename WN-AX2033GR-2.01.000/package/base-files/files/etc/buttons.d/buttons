config button 'reset'
        option button 'reset'
        option gpio_num '6'
        option active_low '1'
        option action 'released'
        option handler 'sh /etc/btnd/reset_hold.sh'
        option min '3000'
        option max '4294967295'
        option action2 'pressed'
        option handler2 'sh /etc/btnd/reset_led.sh'
        option min2 '3000'
        option max2 '4294967295'

config button 'wps'
        option button 'wps'
        option gpio_num '18'
        option active_low '1'
        option action 'released'
        option handler 'sh /etc/btnd/wps_click.sh'
        option min '100'
        option max '3000'
        option action2 'released'
        option handler2 'sh /etc/btnd/wps_copy.sh'
        option min2 '3000'
        option max2 '6000'
        option action3 'pressed'
        option handler3 'sh /etc/btnd/wps_led.sh'
        option min3 '100'
        option max3 '3000'

config button 'auto_bridge_on'
        option button 'auto_bridge_on'
        option gpio_num '7'

config button 'auto_bridge_off'
        option button 'auto_bridge_off'
        option gpio_num '14'