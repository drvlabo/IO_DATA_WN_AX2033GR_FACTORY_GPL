config led 'power'
        option name 'power'
        option sysfs 'mstc:green:power:active_low'
        option default '1'
        option delayon '100'
        option delayoff '100'
        option active_low '1'
        option color 'green'
        option gpio_num '9'
        option command '/usr/bin/gpio'

config led 'wps'
        option name 'wps'
        option sysfs 'mstc:green:wps:active_low'
        option default '0'
        option delayon '200'
        option delayoff '100'
        option active_low '0'
        option color 'green'
        option gpio_num '8'
        option command '/usr/bin/gpio'

config led 'wps_error'
        option name 'wps_error'
        option sysfs 'mstc:green:wps_error:active_low'
        option default '0'
        option delayon '100'
        option delayoff '100'
        option active_low '0'
        option color 'green'
        option gpio_num '8'
        option command '/usr/bin/gpio'

config led 'wps_copy'
        option name 'wps_copy'
        option sysfs 'mstc:green:wps_copy:active_low'
        option default '0'
        option delayon '500'
        option delayoff '500'
        option active_low '0'
        option color 'green'
        option gpio_num '8'
        option command '/usr/bin/gpio'
